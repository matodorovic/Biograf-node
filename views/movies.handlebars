    <main class="movies sec">
      <section class="movies-content sec-cont">
        <div>
          <h2 class="movies-h2"></h2>

        </div>
        <ul class="list-styling">
            {{#each movies }}
                <li>
                    <a href='/movies/{{ this.id }}' >
                        <img src={{ this.attributes.image.url }} aria-label={{ this.attributes.title }}/>
                    </a>
                </li>
            {{/each}}
        </ul>
      </section>
    </main>